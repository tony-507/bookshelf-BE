    "helmet": "3.22.0",
    "knex": "0.20.13",
    "sqlite3": "4.1.1",
    "@types/express": "4.17.6",
    "typescript": "3.8.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.3"
  },
  "engines": {
    "node": "14.x",
    "npm": "6.14.x"
  }
