{
  "name": "bookshelf-app-BE",
  "version": "1.0.0",
  "description": "Backend express-sqlite server for bookshelf-app",
  "private": false,
  "license": "MIT",
  "browserslist": [
    "last 7 versions",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "scripts": {
